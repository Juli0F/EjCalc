<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Operacion</servlet-name>
        <servlet-class>com.calcular.Operacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Condor</servlet-name>
        <servlet-class>operacion.Condor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>operacion.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServletR</servlet-name>
        <servlet-class>operacion.Operacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Calcular</servlet-name>
        <servlet-class>operacion.Calcular</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>Operacion</servlet-name>
        <url-pattern>/Operacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Condor</servlet-name>
        <url-pattern>/Condor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServletR</servlet-name>
        <url-pattern>/NewServletR</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Calcular</servlet-name>
        <url-pattern>/Calcular</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
